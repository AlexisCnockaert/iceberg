SmalltalkCISpec {
  #loading : [
    SCIMetacelloLoadSpec {
      #baseline : 'Iceberg',
      #directory : 'src',
      #onConflict : #useIncoming,
      #onUpgrade : #useIncoming,
      #ignoreImage : true,
      #onWarningLog : true,
      #load : [ 'development' ]      
    }
  ],
  #preTesting : [ '.github/scripts/preTesting.st' ],
  #testing : {
  	#packages : [ 
  		'System-Settings-Tests.*',
  		'ReleaseTests.*' ] 
  }  
}
